/*Code used to control a DC motor using the L298N Motor Driver Module.Author: Max Altenhofen*///Set pin shortcut valuesint en = 13;int in1 = 12;int in2 = 11;void setup() {  //initialize pins    pinMode(en , OUTPUT);    pinMode(in1 , OUTPUT);    pinMode(in2 , OUTPUT);  //Set default outputs of those pins    digitalWrite(in1, LOW);    digitalWrite(in2, LOW);}void loop() {  //Set enable to full power    analogWrite(en, 50);  //Set the switches to move forward    digitalWrite(in1, HIGH);    digitalWrite(in2, LOW);  //Move Forward 5 seconds    delay(5000);  //Set enable to move at halfspeed    analogWrite(en, 100);  //Set switches to move backward    digitalWrite(in1, LOW);    digitalWrite(in2, HIGH);  //Move Backward 5 seconds    delay(5000);}